# Base directory where your files and configs live
DATA_FOLDER=/var/www/prod/gen-ai-agents/n8n-builder

# Domain info for Caddy + SSL + n8n
DOMAIN_NAME=rubicon.ma
SUBDOMAIN=genai
SSL_EMAIL=contact@rubicon.ma
WEBHOOK_URL=https://genai.rubicon.ma/

# Timezone for n8n
GENERIC_TIMEZONE=Africa/Casablanca

# =========================
# PostgreSQL Credentials
# =========================
POSTGRES_USER=n8n
POSTGRES_PASSWORD=securepassword123
POSTGRES_DB=n8n_data

# =========================
# Required by n8n for encryption and login
# =========================
N8N_ENCRYPTION_KEY=your_super_secret_32_characters_key
N8N_USER_MANAGEMENT_JWT_SECRET=another_super_secret_token

# Optional proxy config if behind a reverse proxy
N8N_PROTOCOL=https
N8N_PORT=5678
