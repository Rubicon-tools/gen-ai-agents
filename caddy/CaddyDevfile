dev.genai-docs.rubicon.ma {
	tls /etc/caddy/dev.genai-docs.rubicon.ma.pem /etc/caddy/dev.genai-docs.rubicon.ma-key.pem
	reverse_proxy outline-app:3000 {
		header_down Strict-Transport-Security "max-age=31536000"
	}
}

